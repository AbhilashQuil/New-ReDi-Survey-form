{
  "display": "form",
  "components": [
    {
      "type": "content",
      "html": "<b>Please rate your proficiency level for each of the following skills:</b>"
    },
    {
      "type": "datagrid",
      "key": "skillsMatrix",
      "label": "",
      "hideLabel": true,
      "addAnother": false,
      "addAnotherPosition": "bottom",
      "defaultOpen": true,
      "layoutFixed": false,
      "enableRowGroups": false,
      "initEmpty": false,
      "components": [
        {
          "type": "textfield",
          "key": "skill",
          "label": "Skill",
          "disabled": true,
          "tableView": true,
          "input": true
        },
        {
          "type": "number",
          "key": "proficiency",
          "label": "Proficiency Level (0-7)",
          "tableView": true,
          "min": 0,
          "max": 7,
          "step": 1,
          "defaultValue": 0,
          "validate": {
            "required": true
          },
          "input": true
        }
      ],
      "defaultValue": {{secSkillsData}}
    },
    {
      "type": "button",
      "action": "submit",
      "label": "Next",
      "theme": "primary"
    }
  ]
}
